package File;

import java.io.File;
import java.io.IOException;

public class XoaFile {
	public static void main(String[] args) {
		File f = new File("F:\\HT2001");
		File f2 = new File("F:\\HT2001\\vidu.txt");
		f.mkdir();
		try {
			f2.createNewFile();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
	}
}
